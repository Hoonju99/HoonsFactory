	<div id="result"></div>
<script>

var url = "getRealName";
$(function(){
	$.ajax(url)
	.done(function(result){
		for(bank of result){
			$("<div>").addClass("acc").data("usenum", bank.fintech_use_num)
			.append( $("<span>").html(bank.fintech_use_num))
			.append( $("<span>").html(bank.bank_name))
			.append( $("<input>").val(bank.account_alias))
		}
	})


</script>

